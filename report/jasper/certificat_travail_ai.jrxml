FULL JOIN hr_gp_activity hgac ON hgc.activity=hgac.id
WHERE hgc.employee_id=$P{CUSTOM_EMPL_ID} and hgpd.name='EMPLOI_CTT' and hgac.establishment=$P{ETAB}]]>
